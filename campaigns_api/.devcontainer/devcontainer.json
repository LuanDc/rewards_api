{
  "name": "Campaigns API",
  "dockerComposeFile": ["../../docker-compose.dev.yml"],
  "service": "app",
  "workspaceFolder": "/workspace/campaigns_api",
  "forwardPorts": [4000],
  "postCreateCommand": "mix deps.get && mix ecto.setup && mix run priv/repo/seeds.exs",
  "customizations": {
    "vscode": {
      "extensions": [
        "JakeBecker.elixir-ls"
      ],
      "settings": {
        "editor.formatOnSave": true,
        "[elixir]": {
          "editor.defaultFormatter": "JakeBecker.elixir-ls"
        }
      }
    }
  }
}
